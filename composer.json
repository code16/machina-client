{
    "name": "code16/machina-client",
    "description": "Client package for code16/machina",
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6|^10.0|^11.0",
        "orchestra/testbench": "8.*|^9.0|^10.0",
        "code16/machina": "^1.0"
    },
    "autoload": {
       "psr-4": {
            "Code16\\MachinaClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\MachinaClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\MachinaClient\\MachinaClientServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability" : "dev",
    "prefer-stable": true
}
