{
    "name": "code16/machina-client",
    "description": "Client package for code16/machina",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "guzzlehttp/guzzle": "^6.3|^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.*|9.5.*",
        "orchestra/testbench": "6.*|7.*",
        "code16/machina": "^1.0"
    },
    "autoload": {
       "psr-4": {
            "Code16\\MachinaClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\MachinaClient\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\MachinaClient\\MachinaClientServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "minimum-stability" : "dev",
    "prefer-stable": true
}
